{
    "liveSassCompile.settings.formats":[
       {
           "format": "expanded",
           "extensionName": ".css",
           "savePath": "/assets/css"
       },
       {
           "extensionName": ".min.css",
           "format": "compressed",
           "savePath": "/assets/css"
       }
   ],
   "liveSassCompile.settings.excludeList": [
      "**/node_modules/**",
      "/.vscode/**",
      "**",
      "/bootstrap-4.3.1/**",
      "/assets/css/custom_scss/**",

   ],
   "liveSassCompile.settings.generateMap": true,
   "liveSassCompile.settings.autoprefix": null
}